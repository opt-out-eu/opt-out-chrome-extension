{
  "name": "Opt Out (one click GDPR erasure requests)",
  "version": "1.0.2",
  "description": "Get thousands of organisations to erase your personal data. Easily send GDPR Erasure request (right to be forgotten).",
  "content_security_policy": "default-src 'none'; script-src 'self'; connect-src https://api.opt-out.eu/companies",
  "background": {
    "scripts": [      
      "psl.min.js",
      "background.js"
    ]
  },
  "browser_action": {
      "default_icon": "icon-16.png",
      "default_title": "Opt Out"
  },
  "permissions": [
      "tabs",
      "https://api.opt-out.eu/companies*"
  ],
  "icons" : {
    "48" : "icon-48.png",
    "128" : "icon-128.png"
  },
  "manifest_version": 2
}
